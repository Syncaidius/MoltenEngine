        /// <summary>
        /// Calculates the natural logarithm of the specified quaternion.
        /// </summary>
        /// <param name="value">The quaternion whose logarithm will be calculated.</param>
        /// <param name="result">When the method completes, contains the natural logarithm of the quaternion.</param>
        public static <#=fName#> Logarithm(ref <#=fName#> value)
        {
            <#=fName#> result;

            if (Math.Abs(value.W) < 1.0)
            {
                <#=d.DT#> angle = <#=fpCast#>Math.Acos(value.W);
                <#=d.DT#> sin = <#=fpCast#>Math.Sin(angle);

                if (!<#=mathHelper#>.IsZero(sin))
                {
                    <#=d.DT#> coeff = angle / sin;
                    result.X = value.X * coeff;
                    result.Y = value.Y * coeff;
                    result.Z = value.Z * coeff;
                }
                else
                {
                    result = value;
                }
            }
            else
            {
                result = value;
            }

            result.W = 0.0<#=d.FPL#>;
            return result;
        }

