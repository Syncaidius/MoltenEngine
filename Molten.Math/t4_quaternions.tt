<#@ template debug="false" hostspecific="true" language="C#" #>

<#@ include file="t4_header.tt" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Text.RegularExpressions" #>
<#@ import namespace="System.Collections.Generic" #>
<#@ output extension=".txt" #>

<#
string outputPath = this.Host.ResolvePath("");
List<TypeData> _def = ReadJsonDef<TypeData>(outputPath, "quaternion_defs.json");
string _pMap = "xyzw";
string _pMapUpper = "XYZW";

foreach(TypeData d in _def)
{
	if(!d.IsFPType)
	{
		continue;
	}

	string fName = $"{d.Name}{d.Postfix}";
    string vectorName = $"Vector3{d.Postfix}";
	string fn = $"{fName}";
	string fpCast = d.DT == "float" ? "(float)" : (d.DT == "double" ? "" : $"({d.DT})");
	string mathHelper = d.DT == "double" ? "MathHelperDP" : "MathHelper";
	string math = d.FP == "float" ? "MathF" : "Math";
    string subNamespace = "";
    string subDir = "";

    switch(d.Bits){
        case 64: 
            subNamespace = ".DoublePrecision"; 
            subDir = "/DoublePrecision";
            break;

        case 128: 
            subNamespace = ".QuadPrecision"; 
            subDir = "/QuadPrecision";
            break;

        default:
            subDir = "/SinglePrecision";
            break;
    }
#>
