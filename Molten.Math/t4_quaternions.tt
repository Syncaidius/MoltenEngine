        /// <returns>An array of three <#=See(fName)#> that represent control points for spherical quadrangle interpolation.</returns>
        public static <#=fName#>[] SquadSetup(ref <#=fName#> value1, ref <#=fName#> value2, ref <#=fName#> value3, ref <#=fName#> value4)
        {
            <#=fName#> q0 = (value1 + value2).LengthSquared() < (value1 - value2).LengthSquared() ? -value1 : value1;
            <#=fName#> q2 = (value2 + value3).LengthSquared() < (value2 - value3).LengthSquared() ? -value3 : value3;
            <#=fName#> q3 = (value3 + value4).LengthSquared() < (value3 - value4).LengthSquared() ? -value4 : value4;
            <#=fName#> q1 = value2;

            <#=fName#> q1Exp = Exponential(ref q1);
            <#=fName#> q2Exp = Exponential(ref q2);

            <#=fName#>[] results = new <#=fName#>[3];
            results[0] = q1 * Exponential(-0.25f * (Logarithm(q1Exp * q2) + Logarithm(q1Exp * q0)));
            results[1] = q2 * Exponential(-0.25f * (Logarithm(q2Exp * q3) + Logarithm(q2Exp * q1)));
            results[2] = q2;

            return results;
