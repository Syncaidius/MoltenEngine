        public static void Concatenate(ref <#=fName#> a, ref <#=fName#> b, out <#=fName#> result)
        {
            <#=d.DT#> aX = a.X;
            <#=d.DT#> aY = a.Y;
            <#=d.DT#> aZ = a.Z;
            <#=d.DT#> aW = a.W;
            <#=d.DT#> bX = b.X;
            <#=d.DT#> bY = b.Y;
            <#=d.DT#> bZ = b.Z;
            <#=d.DT#> bW = b.W;

            result.X = aW * bX + aX * bW + aZ * bY - aY * bZ;
            result.Y = aW * bY + aY * bW + aX * bZ - aZ * bX;
            result.Z = aW * bZ + aZ * bW + aY * bX - aX * bY;
            result.W = aW * bW - aX * bX - aY * bY - aZ * bZ;
        }

