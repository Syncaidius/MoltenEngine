            <#=d.DT#> lengthSquared = axis.LengthSquared();
            if (lengthSquared > 1e-14f)
            {
                axis = axis / <#=math#>.Sqrt(lengthSquared);
                angle = 2<#=d.FPL#> * <#=math#>.Acos(<#=d.DT#>.Clamp(qw, -1, 1));
