{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**",
            "**/Molten.Examples.Desktop.Windows.csproj",
            "**/Molten.Examples.Common.csproj",
            "**/Molten.Benchmarks.csproj"
          ]
        }
      ],
      "dest": "api",
      "shouldSkipMarkup": true,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net7.0"
      }
    }
  ],
  "build": {
    "content":
      [
        {
          "files": [
            "api/**.yml", 
            "api/**.md",
            "articles/**.yml",
            "articles/**.md"
          ]
        },
        {
          "files": [
            "toc.yml", 
            "*.md"
          ]
        }
      ],
    "resource": [
        {
          "files": [
            "images\\logo.svg"
          ]
        }
    ],
    "externalReference": [
    ],
    "globalMetadata": {
      "_appTitle": "Molten Engine",
      "_appLogoPath": "images/logo.svg",
      "_appFooter": "<span>Copyright Â© 2023 James Yarwood<br>Generated by <strong>DocFX</strong></span>",
      "_disableContribution": true
    },
    "dest": "../docs/",
    "template": "molten-template",
    "sitemap": {
      "baseUrl": "https://syncaidius.github.io/MoltenEngine/",
      "priority": 0.1,
      "changefreq": "monthly"
    }
  }
}